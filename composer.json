{
    "name": "edbizarro/laravel-facebook-ads",
    "type": "library",
    "description": "Facebook & Instagram Ads for Laravel 5.6",
    "license": "MIT",
    "keywords": [
        "laravel",
        "facebook",
        "instagram",
        "ads",
        "insights",
        "sdk",
        "php"
    ],
    "authors": [
        {
            "name": "Eduardo Bizarro",
            "email": "edbizarro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.13",
        "facebook/php-business-sdk": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~7.0",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Edbizarro\\LaravelFacebookAds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFacebookAds\\Test\\": "tests/"
        }
    },
    "config":{
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true,
    "extra": {
      "laravel": {
        "providers": [
          "Edbizarro\\LaravelFacebookAds\\Providers\\LaravelFacebookServiceProvider"
        ]
      }
    }
}
