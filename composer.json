{
    "name": "edbizarro/laravel-facebook-ads",
    "type": "library",
    "description": "Facebook & Instagram Ads for Laravel 5.6+",
    "keywords": [
        "laravel",
        "facebook",
        "instagram",
        "ads",
        "insights",
        "sdk",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eduardo Bizarro",
            "email": "edbizarro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.13",
        "facebook/php-business-sdk": "^3.3 || ^4.0 || ^5.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~7.3 || ~8.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Edbizarro\\LaravelFacebookAds\\Providers\\LaravelFacebookServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Edbizarro\\LaravelFacebookAds\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFacebookAds\\Test\\": "tests/"
        }
    },
    "prefer-stable": true
}
